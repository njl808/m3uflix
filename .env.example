# IPTV Player Environment Configuration
# Copy this file to .env and update with your actual values

# Xtream Codes API Configuration
XTREAM_URL=http://your-iptv-server.com
XTREAM_USERNAME=your_username
XTREAM_PASSWORD=your_password

# Database Configuration (for production)
DATABASE_URL=postgresql://username:password@localhost:5432/iptv_player

# Session Security
SESSION_SECRET=your-super-secret-session-key-change-this-in-production

# Optional: Timezone
TZ=UTC

# Optional: Application Port (default: 5000)
PORT=5000

# Optional: Node Environment
NODE_ENV=production